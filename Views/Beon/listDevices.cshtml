@model List<EnvironmentPayload>

@{
    ViewData["Title"] = "listDevices";
    Layout = "~/Views/Shared/_LayoutBeonPrincipal.cshtml";
}
<div class="border p-3">
    <h3 class="text-left  ">Tracker List</h3>
</div>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Device</th>
            <th scope="col">Last contact</th>
        </tr>
    </thead>
    <tbody>
       
      
        @foreach (EnvironmentPayload environment in Model)
        {
        <tr>
            <th scope="row"> <a asp-controller="Beon" asp-action="TestDashboard" asp-route-IdDevice="@environment.IdDevice"> @environment.IdDevice</a></th>
            <td scope="row"><a asp-controller="Beon" asp-action="TestDashboard" asp-route-IdDevice="@environment.IdDevice"> @environment.TimestampEvent</a> </td>
        </tr>
        }
    </tbody>
</table>
